                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
	CAST(ISNULL(b.BusinessId, 0) as decimal(18, 0)) as BusinessId,
	CAST(LEFT(ISNULL(b.BusinessName, ''), 25) as varchar(25)) as BusinessName,
	CAST(ISNULL(b.AccountId, '') as varchar(20)) as AccountId,
	CAST(ISNULL(p.ProjectId, 0) as decimal(18, 0)) as ProjectId,
	CAST(LEFT(ISNULL(REPLACE(REPLACE(p.Name, NCHAR(8211), '-'), NCHAR(8212), '-'), ''), 25) as varchar(25)) as ProjectName,
	CAST('' as varchar(10)) as ProjectReferenceId,
	CAST(ISNULL(b.ERPAccountId, 0) as decimal(8, 0)) as ERPAccountNumber,
	CAST(0 as decimal(8, 0)) as TradePartnerNumber,
	CAST(ISNULL(q.QuoteId, 0) as decimal(18, 0)) as QuoteId,
	CAST('' as varchar(10)) as QuoteReferenceId
FROM 
	Projects p 
	LEFT JOIN Users u
		ON p.OwnerId = u.UserId
	LEFT JOIN Businesses b
		ON u.BusinessId = b.BusinessId
	LEFT JOIN Quotes q
		ON p.ProjectId = q.ProjectId
			AND q.Active = 1
WHERE
	1 = 1
	AND ISNULL(p.Deleted, 0) = 0</property>
