                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT distinct o.OrderId 
	FROM Orders o(NOLOCK)
	 INNER JOIN OrderStatusTypes ost(NOLOCK)
	  ON o.OrderStatusTypeId = ost.OrderStatusTypeId
	 INNER JOIN OrderItems oi(NOLOCK)
	  ON o.OrderId = oi.OrderId
	 INNER JOIN Businesses b(NOLOCK)	
	  ON o.BusinessId = b.BusinessId
	 INNER JOIN Addresses a(NOLOCK)
	  ON o.ShipToAddressId = a.AddressId
	 LEFT OUTER JOIN [dbo].[States] s(NOLOCK)
	  ON a.StateId = s.StateId	
	 INNER JOIN [dbo].[Quotes] q(NOLOCK)
	  ON o.QuoteId = q.QuoteId
	 INNER JOIN [dbo].[Projects] p(NOLOCK)
	  ON q.ProjectId = p.ProjectId
WHERE ([ERPPOKey] IS NULL OR [ERPPOKey] =0)
	AND ost.[OrderStatusTypeId] = 2 --ost.Name = 'Submitted'</property>
