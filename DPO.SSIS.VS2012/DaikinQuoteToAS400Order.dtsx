                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT  
	Po_Key =
		[ERPPOKey]
	, o.OrderId
	, NoOfItems = 
		Count(oi.LineSequence)
	, AwaitingCSRStatusId = 
				(SELECT TOP 1 OrderStatusTypeId
					FROM OrderStatusTypes
					 WHERE OrderStatusTypeId = 3 -- name='AwaitingCSR'
				)
	FROM Orders o(NOLOCK)
	 INNER JOIN OrderStatusTypes ost(NOLOCK)
	  ON o.OrderStatusTypeId = ost.OrderStatusTypeId
	 INNER JOIN OrderItems oi(NOLOCK)
	  ON o.OrderId = oi.OrderId
	 INNER JOIN Businesses b(NOLOCK)	
	  ON o.BusinessId = b.BusinessId
	 INNER JOIN Addresses a(NOLOCK)
	  ON o.ShipToAddressId = a.AddressId
	 LEFT OUTER JOIN [dbo].[States] s(NOLOCK) --need to change to Inner join
	  ON a.StateId = s.StateId	
	 INNER JOIN [dbo].[Quotes] q(NOLOCK)
	  ON o.QuoteId = q.QuoteId
	 INNER JOIN [dbo].[Projects] p(NOLOCK)
	  ON q.ProjectId = p.ProjectId
	 INNER JOIN [dbo].[Products] pd
	  ON oi.ProductId = pd.ProductId	  	
	WHERE 1=1
		AND NULLIF([ERPPOKey],0) IS NOT NULL
		AND ERPStatus = 'I'
		AND ost.OrderStatusTypeId = 2 -- ost.Name = 'Submitted'		
	GROUP BY  o.OrderId,[ERPPOKey]</property>
