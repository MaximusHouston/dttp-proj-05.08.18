                  name="Pending Order Details"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT  
--Po Key	E_POKEY	P	6
	Po_Key =
		[ERPPOKey]
--Crm Active Id		E_CRMACT	A	50
	, CRMAccountID =
		CAST(ISNULL(b.AccountId,'') AS VARCHAR(50))		
	, ERP_Po_Key =
		CAST(ISNULL([ERPPOKey],0) AS DECIMAL(6,0))
--Line Seq	X_LINSEQ	P	8	0	
	, Line_Seq = 
		ISNULL(oi.LineSequence,0)
--Item No	X_ITNBR	A	15	 	Required
	, Item_No =
		CAST(ISNULL(oi.ProductNumber,'')AS VARCHAR(15))
--Cust Itm No	X_CUITMN	A	30	
	, Cust_Itm_No =
		CAST('' AS VARCHAR(30))  	
--Order Qty	X_ORDQTY	P	12	3	Required
	, Order_Qty =
		ISNULL(oi.Quantity,0)
--Selling Pr	X_SELPR	P	16	3
	, Selling_Price =
		ISNULL(oi.[NetPrice],0) 	
--Net Sell Amt	X_SELAMT	P	16	3	
	, Net_Sell_Amount = 
		ISNULL(oi.[ExtendedNetPrice],0)
--Item Description	X_DESCR	A	80	 	
	, Item_Description = 
		CAST(pd.Name AS VARCHAR(80))
	, o.OrderId
--X_FILL5 
	, DiscountPercentage = 
		oi.DiscountPercentage
	FROM Orders o(NOLOCK)
	 INNER JOIN OrderStatusTypes ost(NOLOCK)
	  ON o.OrderStatusTypeId = ost.OrderStatusTypeId
	 INNER JOIN OrderItems oi(NOLOCK)
	  ON o.OrderId = oi.OrderId
	 INNER JOIN Businesses b(NOLOCK)	
	  ON o.BusinessId = b.BusinessId
	 INNER JOIN Addresses a(NOLOCK)
	  ON o.ShipToAddressId = a.AddressId
	 LEFT OUTER JOIN [dbo].[States] s(NOLOCK) --need to change to Inner join
	  ON a.StateId = s.StateId	
	 INNER JOIN [dbo].[Quotes] q(NOLOCK)
	  ON o.QuoteId = q.QuoteId
	 INNER JOIN [dbo].[Projects] p(NOLOCK)
	  ON q.ProjectId = p.ProjectId
	 INNER JOIN [dbo].[Products] pd
	  ON oi.ProductId = pd.ProductId	
	WHERE 1=1
		AND NULLIF([ERPPOKey],0) IS NOT NULL
		AND ost.[OrderStatusTypeId] = 2 --ost.Name = 'Submitted'
	ORDER BY [ERPPOKey],oi.LineSequence</property>
