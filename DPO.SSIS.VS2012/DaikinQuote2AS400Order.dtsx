    private string GetKeyNum()
    {
        //System.Windows.Forms.MessageBox.Show(this.Connections.AS400Connection.ConnectionString);
        string newPoKey = string.Empty;
        string commandText = "{{ CALL {0}.DAIKIN_PRC(?) }}";
        string gmLibCoreT = this.Variables.POKEYPGMLOC;

        try
        {
            //using (as400Connection = new OleDbConnection(this.Connections.AS400Connection.ConnectionString))
            using (as400Connection = new OleDbConnection(this.Variables.ISeriesConnectionString))
            {
                OleDbCommand cmd = new OleDbCommand();
                cmd.CommandType = System.Data.CommandType.Text;
                //cmd.CommandText = "{{ CALL GMLIBCORET.DAIKIN_PRC(?) }}";
                cmd.CommandText = string.Format(commandText, gmLibCoreT);
                OleDbParameter param1 = new OleDbParameter("param1", "");
                param1.DbType = System.Data.DbType.AnsiStringFixedLength;
                param1.Size = 6;
                param1.Direction = System.Data.ParameterDirection.InputOutput;
                cmd.Parameters.Add(param1);
                cmd.Connection = as400Connection;
                if (as400Connection.State != System.Data.ConnectionState.Open)
                {
                    as400Connection.Open();
                }
                cmd.ExecuteScalar();

                newPoKey = param1.Value.ToString();
            }


        }
        catch (System.Exception ex)
        {
            newPoKey = string.Empty;
            //System.Windows.Forms.MessageBox.Show(ex.Message);
            throw ex;
        }
        finally
        {
            if (as400Connection.State == System.Data.ConnectionState.Open)
                as400Connection.Close();
        }
        return newPoKey;
      
    }

