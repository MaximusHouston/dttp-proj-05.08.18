                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT  
--Po Key	E_POKEY	P	6
	Po_Key =
		o.[ERPPOKey]
--Customer No	E_CANB	P	8
	, Customer_No = 
		ISNULL(b.[ERPAccountId],0)

--Po No	E_PONO	A	22
	, Po_No = 
		CAST(o.PONumber AS VARCHAR(22))

--Po Date	E_PODATE	P	8
	, Po_Date =
		CAST(ISNULL( CONVERT (DECIMAL(8),REPLACE(CONVERT(CHAR(10), GETDATE(),126), '-', '')), 0) AS VARCHAR(8))
--Request Date	E_RQDATE	P	8
	, Request_Date= 
		CAST(ISNULL( CONVERT (DECIMAL(8),REPLACE(CONVERT(CHAR(10), o.EstimatedReleaseDate,126), '-', '')), 0) AS VARCHAR(8))

--Terms Code	E_TERMCD	A	2
	, Terms_Code = 
		''
--Order Type	E_ORDTYPE	A	2
	, Order_Type =
		'DK'
--Ship To Name	E_SHPTONM	A	30
	, Ship_To_Name =
		CAST(LEFT(ISNULL(b.BusinessName,''),30) AS VARCHAR(30))	--*********TODO

--Ship To Address	E_SHPTOADR	A	30
	, Ship_To_Address =
		CAST(LEFT(ISNULL(a.AddressLine1,''),30) AS VARCHAR(30))

--Ship To Adr 2	E_ADRR2	A	30
	, Ship_To_Address2 =
		CAST(LEFT(ISNULL(a.AddressLine2,''),30) AS VARCHAR(30))

--Ship To City	E_SHPTOCTY	A	30
	, Ship_To_City =
		CAST(LEFT(ISNULL(a.Location,''),30) AS VARCHAR(30))	--*********TODO

--Ship To State	E_SHPTOST	A	2
	, Ship_To_State =
		CAST(ISNULL(s.Code,'') AS VARCHAR(2))

--Ship To Zip	E_SHPTOZP	A	10
	, Ship_To_Zip =
		CAST(ISNULL(a.PostalCode,'') AS VARCHAR(10))

--Shipping Instr	E_SHPINST	A	30
	, Ship_To_Instr =
		CAST(ISNULL(o.Comments,'') AS VARCHAR(20))

--Contact Name	E_CONTACT	A	25
	, Contact_Name = 
		CAST(LEFT(ISNULL(o.DeliveryContactName,''),20) AS VARCHAR(25))

--Contact No (Phone#)	E_CNTNBR	A	30
	, Contact_No = 
		CAST(LEFT(ISNULL(o.DeliveryContactPhone,''),30) AS VARCHAR(30))

--Total Amount	E_TOTAMT	A	18
	, Total_Amount = 
		CAST('0' AS VARCHAR(18))
--Order Code	E_ORDRCD	A	2
	, Order_Code =
		CAST('' AS VARCHAR(2))
--Status	E_STATUS	A	1
	, Status =
		'I'	--inprocess/ S  suspended
--Ship To	E_SHIPTO	A	8
	, Ship_To =
		CAST('' AS VARCHAR(8))
--Mapics Cmp#	E_MCO	P	2
	, Mapics_Company =
		1

--Created User	E_CUSER	A	10
--Created Time	E_CTIME	A	26
--Updated User	E_UUSER	A	10
--Updated Time	E_UTIME	A	26

--Customer Name	E_CUSNM	A	30
	, Customer_Name =
		CAST(ISNULL(b.[BusinessName],'') AS VARCHAR(30))
--Mapics Ord Nbr	E_ORDNO	A	7
--Po Ack Flag	E_PAFLG	A	1
--Upd Date - 8	E_UPDDT	A	8

--Discount%	E_DISCNT	P	5
	, Discount_Percent = 
		o.TotalDiscountPercent

--Comment1	E_COMT	A	2000
--Comments	E_COMT	A	865
	, Comments = 
		CAST(ISNULL(o.Comments,'') AS VARCHAR(2000))
    
--Filler1	E_FILL1	A	50
--Project Name		E_PRJNM	A	25
	,  Project_Name = 
		CAST(ISNULL(p.name,'') AS VARCHAR(25))

--Filler2	E_FILL2	A	50
--Business Name		E_BUSNM	A	25
	, BusinessName = 
		CAST(ISNULL(b.BusinessName,'')AS VARCHAR(25))

--Filler3	E_FILL3	P	18
--Business Id		E_BUSSID	S	19
	, Businessid = 
		b.BusinessId
--Filler4	E_FILL4	P	18
--Quote Id		E_QUOTEID	S	19
	, Quote_ID =
		o.QuoteId
--Filler5	E_FILL5	P	18
--Project Id		E_PRJID	S	19
	, ProjectId	 =
		p.ProjectId

--Filler11	E_FILL11	A	20
--Crm Active Id		E_CRMACT	A	50
	, CRMAccountID =
		CAST(ISNULL(b.AccountId,'') AS VARCHAR(50))
--(00866-01890)		E_COMT	A	1025
--(01891-02000)		E_COMT	A	110
--Project Ref Id		E_PRJREFID	A	10
--Trading Partner#		E_TRDN	S	8
--Quote Ref Id		E_QUOTERF	A	12
, ERP_Po_Key =
		CAST(ISNULL([ERPPOKey],0) AS DECIMAL(6,0))
, o.OrderId
	FROM Orders o(NOLOCK)
	 INNER JOIN OrderStatusTypes ost(NOLOCK)
	  ON o.OrderStatusTypeId = ost.OrderStatusTypeId
	 -- INNER JOIN OrderItems oi(NOLOCK)
	  --ON o.OrderId = oi.OrderId
	 INNER JOIN Businesses b(NOLOCK)	
	  ON o.BusinessId = b.BusinessId
	 INNER JOIN Addresses a(NOLOCK)
	  ON o.ShipToAddressId = a.AddressId
	 LEFT OUTER JOIN [dbo].[States] s(NOLOCK) --need to change to Inner join
	  ON a.StateId = s.StateId	
	 INNER JOIN [dbo].[Quotes] q(NOLOCK)
	  ON o.QuoteId = q.QuoteId
	 INNER JOIN [dbo].[Projects] p(NOLOCK)
	  ON q.ProjectId = p.ProjectId
	WHERE 1=1
		AND NULLIF([ERPPOKey],0) IS NOT NULL
		AND ost.[OrderStatusTypeId] = 2--ost.Name = 'Submitted'
AND EXISTS(SELECT TOP 1 1 FROM OrderItems oi(NOLOCK) WHERE o.[OrderId] = oi.OrderId)</property>
