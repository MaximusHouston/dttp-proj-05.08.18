@model WidgetContainerModel
<li data-templateid="@Model.Widget.TemplateId" data-widgettype="Overview.OverviewWidget" data-renderer="Overview.TextElementRenderer">
    <div class="widget-body">
        @Html.Partial("OverviewWidgetTitle")
        @Html.Partial("OverviewWidgetSettings")
        @Html.Partial("OverviewWidgetErrorScreen")
        <div class="contentbox withfooter total-projects">
            <img src="/Images/total-projects-icon.png" />
            <div>
                <h1><strong class="data">&nbsp;</strong></h1>
                <p>Updated Projects (Financial Year)</p>
            </div>
        </div>
        <div class="contentbox-footer">
            <a href="/v2/#/projects">View all projects</a>
        </div>
    </div>
</li>
